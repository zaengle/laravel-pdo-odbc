{
  "name": "zaengle/laravel-pdo-odbc",
  "description": "PDO ODBC integration for Laravel 7 & 8 framework. Tested for Snowflake connections",
  "type": "library",
  "homepage": "https://github.com/yoramdelangen/laravel-odbc",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrea Abram",
      "email": "andreossido@gmail.com",
      "homepage": "https://github.com/andreossido",
      "role": "Developer"
    },
    {
      "name": "Yoram de Langen",
      "email": "yoramdelangen@gmail.com",
      "homepage": "https://github.com/yoramdelangen",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-odbc": "*",
    "ext-pdo_odbc": "*",
    "illuminate/database": ">= 7.0",
    "illuminate/support": ">= 7.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "LaravelPdoOdbc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelPdoOdbc\\ODBCServiceProvider"
      ]
    }
  }
}
